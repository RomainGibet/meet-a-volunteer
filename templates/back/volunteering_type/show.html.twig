    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ volunteering_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ volunteering_type.name }}</td>
            </tr>
            <tr>
                <th>SlugName</th>
